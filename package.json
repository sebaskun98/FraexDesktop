{
  "name": "FraexDesktop",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "package-win": "electron-packager . FraexDesktop --overwrite --asar=true --platform=win32 --arch=ia32",
    "package-linux": "electron-packager . FraexDesktop --overwrite --asar --platform=linux --arch=x64 --icon=assets/icons/png/1024x1024.png"
  },
  "repository": "https://github.com/sebaskun98/FraexDesktop",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^4.0.2",
    "electron-builder": "^20.38.5",
    "electron-packager": "^13.1.1",
    "electron-rebuild": "^1.8.4"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bcrypt": "^3.0.4",
    "mysql": "^2.16.0",
    "node-php-password": "^0.1.2"
  }
}
